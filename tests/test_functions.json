[
    ["print", "--- TEST FONCTIONS RUST ---"],

    ["function", "add", ["a", "b"], [
        ["return", ["+", ["get", "a"], ["get", "b"]]]
    ]],

    ["set", "x", 10],
    ["set", "y", 32],
    ["set", "result", ["call", "add", ["get", "x"], ["get", "y"]]],
    
    ["print", ["+", "Addition (10 + 32) = ", ["get", "result"]]],

    ["print", "--- TEST RECURSIVITE (Factorielle) ---"],

    ["function", "fact", ["n"], [
        ["if", ["<", ["get", "n"], 2],
            [ 
                ["return", 1] 
            ],
            [ 
                ["return", ["*", ["get", "n"], ["call", "fact", ["-", ["get", "n"], 1]]]] 
            ]
        ]
    ]],

    ["print", "Calcul de fact(5)..."],
    ["set", "f5", ["call", "fact", 5]],
    ["print", ["+", "Resultat : ", ["get", "f5"]]]
]