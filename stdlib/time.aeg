class TimeManager() {
    // Retourne le timestamp actuel en ms
    now() {
        return time_now()
    }
    
    // Helper pour mesurer une dur√©e
    // Usage: var start = Time.now(); ...; var elapsed = Time.elapsed(start);
    elapsed(start_time) {
        return time_now() - start_time
    }
}

var Time = new TimeManager()
